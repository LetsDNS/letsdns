[DEFAULT]
basedir = /path/to/certs
keyfile = /etc/letsdns/ddns-keys.json
nameserver = 111.222.333.444
ttl = 60

[Example Domain]
action = tlsa
domain = example.com
certificate = ${basedir}/ham/cert.pem

[Example Subdomain]
action = tlsa
domain = lists.example.com
certificate = ${basedir}/eggs/cert.pem
